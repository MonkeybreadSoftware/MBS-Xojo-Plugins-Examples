		Sub Open()
		  dim f as folderItem
		  
		  f=FindFile("testbild.tif")
		  if f = nil or not f.exists then
		    me.visible=false
		  end if
		End Sub
	#tag EndEvent
	#tag Event
		Sub Action()
		  dim p as picture
		  dim f as folderItem
		  
		  f=FindFile("testbild.tif")
		  p=f.openaspicture
		  
		  if p<>Nil then
		    original.graphics.drawPicture p,0,0
		    update
		  end if
		End Sub
