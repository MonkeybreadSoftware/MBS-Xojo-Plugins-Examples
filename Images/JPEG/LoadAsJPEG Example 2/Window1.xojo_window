#tag WindowCode
	#tag Event
		Sub DropObject(obj As DragItem, action As Integer)
		  dim p as picture
		  dim j as JPEGImporterMBS
		  dim s as String
		  
		  do
		    if obj.folderItemavailable then
		      j=new JPEGImporterMBS
		      j.file=obj.folderItem
		      j.Import
		      
		      err.text=j.ErrorMessage
		      
		      backdrop=j.Picture
		      Title=j.File.DisplayName+": "+str(j.Width)+" x "+str(j.Height)+" @Â "+str(j.CurrentDepth)+" bits"    // <-- CONVERTED
		      
		      select case j.ResolutionUnit
		      case 0
		        s=" dots/?"
		      case 1
		        s=" dots/inch"
		      case 2
		        s=" dots/cm"
		      end Select
		      
		      res.text=str(j.HorizontalResolution)+" x "+str(j.VerticalResolution)+s
		    end if
		  loop until not obj.nextItem
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  
		  me.acceptfileDrop "image/jpeg"
		End Sub
	#tag EndEvent


#tag EndWindowCode

