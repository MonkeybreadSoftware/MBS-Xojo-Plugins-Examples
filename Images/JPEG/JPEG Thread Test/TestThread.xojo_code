#tag Class
Protected Class TestThread
Inherits Thread
	#tag Event
		Sub Run()
		  do
		    test
		  loop
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub test()
		  dim j as new JPEGImporterMBS
		  
		  j.YieldTicks = 1
		  j.Data = data
		  
		  if j.InitJPEG then
		    dim h as integer =j.Height
		    dim w as integer =j.Width
		    
		    j.FinishJPEG
		  else
		    break
		  end if
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		data As string
	#tag EndProperty


