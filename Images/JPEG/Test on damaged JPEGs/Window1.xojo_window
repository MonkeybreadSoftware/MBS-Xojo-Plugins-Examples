#tag WindowCode
	#tag Event
		Sub Open()
		  dim p as Picture
		  dim s as string
		  
		  p = new Picture(100,100,32)
		  p.Graphics.ForeColor=rgb(255,0,0)
		  p.Graphics.FillRect 0,0,100,100
		  
		  s=PictureToJPEGStringMBS(p,100)
		  
		  data=s
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h1
		Protected data As string
	#tag EndProperty

	#tag Property, Flags = &h1
		Protected p As Picture
	#tag EndProperty


#tag EndWindowCode

#tag Events Timer1
	#tag Event
		Sub Action()
		  dim j as JPEGImporterMBS
		  
		  j=new JPEGImporterMBS
		  
		  j.Data=leftb(data,lenb(data)*rnd+1)
		  j.AllowDamaged=true
		  j.Import
		  p=j.Picture
		  
		  
		  Canvas1.Invalidate
		  
		  Exception
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Canvas1
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  g.ClearRect 0, 0, g.Width, g.Height
		  
		  if p <> nil then
		    g.DrawPicture p, 0, 0
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
