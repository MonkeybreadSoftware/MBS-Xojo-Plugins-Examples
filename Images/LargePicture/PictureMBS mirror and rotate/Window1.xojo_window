#tag WindowCode
	#tag Event
		Sub Open()
		  dim l as Picture
		  
		  l=LogoMBS(500)
		  
		  logo=new PictureMBS(l)
		  
		  Current=new PictureMBS(logo.Width,logo.Height, PictureMBS.ImageFormatRGB)
		  call Current.CopyPixels(logo)
		  
		  update
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub update()
		  canvas1.Backdrop=Current.CopyPicture
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		current As Picturembs
	#tag EndProperty

	#tag Property, Flags = &h0
