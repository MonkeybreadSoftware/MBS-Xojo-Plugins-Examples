#tag Events RotateButton
	#tag Event
		Sub Action()
		  // this is the fill color:
		  dim red   as integer = 255
		  dim green as integer = 0
		  dim blue  as integer = 0
		  dim alpha as integer = 0
		  dim gray  as integer = 0
		  
		  
		  current=Current.Rotate(slider1.Value, red, green, blue, alpha, gray)
		  
		  update
		  
		End Sub
