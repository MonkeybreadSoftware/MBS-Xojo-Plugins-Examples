	#tag Event
		Sub Action()
		  
		  dim logo as Picture
		  
		  logo=logombs(500)
		  
		  dim q as Picture
		  
		  q=New Picture(logo.Width, logo.Height, 32)
		  q.Graphics.DrawPicture logo,0,0
		  
		  p=new PictureMBS(q)
		  
		  Title="Height: "+str(p.Height)
		  
		  
		End Sub
