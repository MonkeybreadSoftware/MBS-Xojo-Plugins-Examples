#tag WindowCode
	#tag Event
		Sub Open()
		  self.Maximize
		  p=new PictureMBS(100,100,PictureMBS.ImageFormatRGB)
		  Title="Height: "+str(p.Height)
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub ShowInfo(p as picturembs)
		  dim lines(-1) as string
		  
		  lines.Append "Width: "+str(p.Width)
		  lines.Append "Height: "+str(p.Height)
		  lines.Append "ImageFormat: "+str(p.ImageFormatString)
		  lines.Append "TotalSize: "+str(p.TotalSize)
		  lines.Append "PixelSize: "+str(p.PixelSize)
		  
		  MsgBox Join(lines,EndOfLine)
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		p As PictureMBS
	#tag EndProperty


