#tag WindowCode
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  Select case asc(key)
		  case 28 'left
		    move -10,0
		    Return true
		  case 29 'right
		    move 10,0
		    Return true
		  case 30 'top
		    move 0,-10
		    Return true
		  case 31 'down
		    move 0,10
		    Return true
		  end Select
		  
		End Function
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub Create()
		  
		  o = new MyOverlay(500,100,200,200)
		  
		  // No picture sert as we use draw event
		  
		  o.hasShadow = false
		  
		  o.Show
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub move(x as integer, y as integer)
		  o.Move o.Left+x,o.top+y
		  
		End Sub
	#tag EndMethod


