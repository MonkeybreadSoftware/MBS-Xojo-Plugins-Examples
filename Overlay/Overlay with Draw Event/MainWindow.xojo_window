#tag Events AnimationTimer
	#tag Event
		Sub Action()
		  if o <> nil then
		    
		    if o.growing then
		      o.pos = o.pos + 1
		      
		      if o.pos > o.Width / 2 then
		        o.growing = false
		      end if
		      
		      o.Update
		      
		    else
		      
		      if o.pos = 0 then
		        o.growing = true
		      else
		        o.pos = o.pos - 1
		        o.Update
		      end if
		    end if
		    
		    
		    
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
