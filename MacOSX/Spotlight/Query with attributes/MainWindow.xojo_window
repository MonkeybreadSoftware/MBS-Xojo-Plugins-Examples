#tag WindowCode
	#tag Event
		Sub Close()
		  if q<>nil then
		    q.stop
		    q=nil
		  end if
		  
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h0
		q As MyQuery
	#tag EndProperty


#tag EndWindowCode

#tag Events List
	#tag Event
		Sub Change()
		  PushButton2.Enabled=me.ListIndex>=0
		  PushButton3.Enabled=me.ListIndex>=0
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events SearchButton
	#tag Event
		Sub Action()
		  if q<>nil then
		    q.stop
		    q=nil
		  end if
		  
		  List.DeleteAllRows
		  
		  dim ValuesList(-1) as string = array(MDItemMBS.kMDItemDisplayName, MDItemMBS.kMDItemFSOwnerUserID)
		  
		  q=new MyQuery(query.text, ValuesList)
		  
		  if q.Handle=0 then
		    
		    dim t as string = ReplaceAll(query.text,"""","")
		    
		    q=new MyQuery("* == """+t+"""wcd || kMDItemTextContent = """+t+"""c", ValuesList)
		  end if
		  
		  if q.Handle=0 then
		    MsgBox "Failed to create query"
		  else
		    q.SetMaxCount 100
		    
		    if q.Execute(q.kMDQueryWantsUpdates) then
		      'ok
		      q.EnableUpdates
		    else
		      MsgBox "Failed to run query"
		    end if
		    
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton1
	#tag Event
		Sub Action()
		  dim list(-1) as Variant = q.ValuesOfAttribute(MDItemMBS.kMDItemDisplayName)
		  dim lines(-1) as string
		  
		  for each v as Variant in List
		    lines.Append v
		  next
		  
		  MsgBox Join(lines,EndOfLine)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton2
	#tag Event
		Sub Action()
		  dim it as MDItemMBS = List.RowTag(List.ListIndex)
		  dim d as Dictionary = it.GetAttributes(it.AllAttributes)
		  
		  dim lines(-1) as string
		  
		  for each k as Variant in d.Keys
		    dim v as Variant = d.Value(k)
		    if v.IsNull then v="<null>"
		    if v.IsArray then v="<array>"
		    lines.Append k+" -> "+v
		  next
		  
		  MsgBox Join(lines,EndOfLine)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton3
	#tag Event
		Sub Action()
		  dim it as MDItemMBS = List.RowTag(List.ListIndex)
		  dim v as Variant = it.GetAttribute(it.kMDItemDisplayName)
		  
		  MsgBox v
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton4
	#tag Event
		Sub Action()
		  
		  
		  dim count as integer = Q.CountOfResultsWithAttributeValue(MDItemMBS.kMDItemFSOwnerUserID, 501)
		  MsgBox str(Count)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events UpdateTimer
	#tag Event
		Sub Action()
		  List.DeleteAllRows
		  
		  dim items() as MDItemMBS = q.Results
		  for each it as MDItemMBS in items
		    List.AddRow it.DisplayName
		    List.RowTag(List.LastIndex) = it
		  next
		  
		  Title = str(UBound(items)+1)+" items"
		End Sub
	#tag EndEvent
