#tag Class
Protected Class MyObserver
Inherits NSNotificationObserverMBS
	#tag Event
		Sub GotNotification(notification as NSNotificationMBS)
		  dim l as listbox = MainWindow.List
		  
		  l.AddRow "Got notification: "+notification.name
		  
		  
		End Sub
	#tag EndEvent


