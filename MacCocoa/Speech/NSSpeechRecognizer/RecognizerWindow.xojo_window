#tag WindowCode
	#tag Event
		Sub Open()
		  s=new MyNSSpeechRecognizerMBS
		  s.DisplayedCommandsTitle = "Realbasic"
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h1
		Protected s As MyNSSpeechRecognizerMBS
	#tag EndProperty


#tag EndWindowCode

#tag Events List
	#tag Event
		Sub Change()
		  if me.ListIndex=-1 then
		    RemoveButton.Enabled=False
		  else
		    RemoveButton.Enabled=true
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events AddButton
	#tag Event
		Sub Action()
		  List.AddRow EditField1.text
		  EditField1.text=""
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events EditField1
	#tag Event
		Sub TextChange()
		  if me.text="" then
		    AddButton.Enabled=false
		  else
		    AddButton.Enabled=true
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events RemoveButton
	#tag Event
		Sub Action()
		  List.RemoveRow(List.ListIndex)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton3
	#tag Event
		Sub Action()
		  dim a(-1) as string
		  dim i,c as integer
		  
		  c=List.ListCount-1
		  redim a(c)
		  for i=0 to c
		    a(i)=List.Cell(i,0)
		  next
		  
		  s.SetCommands a
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton1
	#tag Event
		Sub Action()
		  s.StartListening
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton2
	#tag Event
		Sub Action()
		  s.StopListening
		End Sub
	#tag EndEvent
#tag EndEvents
