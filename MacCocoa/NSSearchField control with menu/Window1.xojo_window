		  log CurrentMethodName
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub TextDidBeginEditing(fieldEditor as NSTextMBS, notification as NSNotificationMBS)
		  log CurrentMethodName
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub KeyUp(Key As String)
		  log CurrentMethodName+": "+key
		  If Asc(Key) = 13 Then
		    Dim t As String = Me.View.StringValue
		    Select Case Me.result
		    Case SearchResult.BeginsWith
		      t = t + "%"
		    Case SearchResult.Contains
		      t = "%" + t + "%"
		    Case SearchResult.EndsWith
		      t = "%" + t
		    End Select
		    StaticText1.Text = t
		  End If
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function KeyDown(Key As String) As Boolean
		  log CurrentMethodName+": "+key
		  
		End Function
	#tag EndEvent
