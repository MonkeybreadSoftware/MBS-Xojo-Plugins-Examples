#tag WindowCode
	#tag Event
		Sub Open()
		  
		  if not TargetCocoa then
		    MsgBox "This example requires Mac OS X 10.5 Cocoa."
		    quit
		  end if
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub log(s as string)
		  
		  Listbox1.AddRow s
		  Listbox1.ScrollPosition = Listbox1.ListCount
		End Sub
	#tag EndMethod


