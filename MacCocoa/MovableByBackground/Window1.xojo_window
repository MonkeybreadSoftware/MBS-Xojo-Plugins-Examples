#tag WindowCode
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  windowFrame=self.NSWindowMBS.frame
		  initialLocation = NSEventMBS.mouseLocation
		  initialLocation.x = initialLocation.x - windowFrame.origin.x
		  initialLocation.y = initialLocation.y - windowFrame.origin.y
		  Return TRUE
		  
		End Function
	#tag EndEvent

	#tag Event
		Sub MouseDrag(X As Integer, Y As Integer)
		  currentLocation=NSEventMBS.mouseLocation
		  newOrigin.x = (currentLocation.x-initialLocation.x)
		  newOrigin.y = (currentLocation.y-initialLocation.y)
		  self.NSWindowMBS.setFrameOrigin(newOrigin)
		  
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  if TargetCocoa then
		    initialLocation = new NSPointMBS
		    currentLocation= new NSPointMBS
		    newOrigin = new NSPointMBS
		    screenFrame = NSScreenMBS.mainScreen.frame
		    windowFrame = self.NSWindowMBS.frame
		  else
		    MsgBox "Cocoa build only"
		    quit
		  end if
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h21
		Private currentLocation As NSPointMBS
	#tag EndProperty

	#tag Property, Flags = &h21
		Private initialLocation As NSPointMBS
	#tag EndProperty

	#tag Property, Flags = &h21
		Private newOrigin As NSPointMBS
	#tag EndProperty

	#tag Property, Flags = &h21
		Private screenFrame As NSRectMBS
	#tag EndProperty

	#tag Property, Flags = &h21
		Private windowFrame As NSRectMBS
	#tag EndProperty


#tag EndWindowCode

