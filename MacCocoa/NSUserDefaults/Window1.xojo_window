#tag Events PushButton1
	#tag Event
		Sub Action()
		  // Load this values.
		  
		  dim w,h As Integer
		  w = UserPrefs.integerForKey("winmain_width")
		  h = UserPrefs.integerForKey("winmain_height")
		  
		  Label3.Text = "Window size: "+Str(w)+" x "+Str(h)
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton2
	#tag Event
		Sub Action()
		  // Store this values.
		  // the default values added in the "LoadPref" method
		  // will be replaced and they won't be returned anymore.
		  
		  UserPrefs.setIntegerValue("winmain_width", Self.Width )
		  UserPrefs.setIntegerValue("winmain_height", Self.Height )
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton3
	#tag Event
		Sub Action()
		  // Load a value that has no key, and no default value.
		  // NSUserDefaults will simply return an empty string
		  // for this key without causing an error.
		  
		  dim path As String = UserPrefs.stringForKey("my_file_path")
		  
		  if path="" then
		    Label4.Text = "File path has not been defined"
		  else
		    Label4.Text = "File path: "+path
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton4
	#tag Event
		Sub Action()
		  dim s As String = trim( TextField1.Text )
		  UserPrefs.setStringValue("my_file_path", s )
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton5
	#tag Event
		Sub Action()
		  Label5.Text = "Calculator view: "+UserPrefs.stringForKey("ViewDefaultsKey")
		End Sub
	#tag EndEvent
#tag EndEvents
