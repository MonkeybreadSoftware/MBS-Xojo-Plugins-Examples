#tag Events pbopenwith
	#tag Event
		Sub Action()
		  dim b as boolean
		  dim prog,ff as folderItem
		  dim sourcef as folderitem
		  dim savedstr as string
		  dim tel, foutteller as integer
		  dim dofile as boolean
		  
		  
		  prog = getfolderitem(stapp.text)
		  
		  if prog = nil or prog.exists=false then
		    msgBox "Could not find application to lauch with"
		    return
		  end if
		  
		  
		  ff = getfolderItem(stdoc.text)
		  if ff = nil or ff.exists=false then
		    msgBox "The selected file can't be found"
		    return
		  end if
		  
		  b = ff.OpenWithAppMBS(prog,true)
		  if b=false then 
		    msgBox "error"
		  end if
		  
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbselapp
	#tag Event
		Sub Action()
		  dim ff as folderItem
		  Dim dlg as OpenDialog
		  dim pth as string
		  
		  dlg=New OpenDialog
		  
		  dlg.Title="Select an application"
		  dlg.Filter="appx"
		  ff=dlg.ShowModal()
		  
		  if dlg.result <> nil then
		    
		    pth = dlg.result.NativePath
		    if right(pth,1)=":" then //for os x apps
		      pth=left(pth,len(pth)-1)
		    end if
		    
		    stapp.text= pth
		  end if
		  
		  
		  
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbseldoc
	#tag Event
		Sub Action()
		  dim f as folderItem
		  
		  f=getopenFolderItem("any")
		  if f <> nil then
		    stdoc.text= f.NativePath
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
