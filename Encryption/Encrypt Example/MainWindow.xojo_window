#tag WindowCode
	#tag Event
		Sub Open()
		  dim s as string
		  dim i as integer
		  dim e as string
		  dim d as string
		  
		  dim m as new EncryptMBS
		  
		  m.init "Passwort"
		  
		  s="Hallo Welt!" 'only 8 char aligent text is encoded.
		  
		  original.text=s
		  
		  s=m.Encode(s)
		  
		  encoded.text=s
		  
		  s=m.Decode(s)
		  
		  decoded.text=s
		  
		  s=m.Key
		  
		  e=""
		  for i=1 to lenb(s)
		    e=e+right("00"+hex(ascb(midb(s,i,1))),2)+" "
		  next
		  
		  thekey.text=e+" ("+format(lenb(s),"0")+" Bytes)"
		End Sub
	#tag EndEvent


#tag EndWindowCode

