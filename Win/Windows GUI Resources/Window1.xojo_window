	#tag EndEvent


	#tag Method, Flags = &h0
		Sub Update()
		  dim w as new WindowsGUIResourcesMBS
		  
		  UserObjectCount.Text = str(w.UserObjectCount)
		  UserObjectPeak.Text = str(w.UserObjectPeak)
		  GDIObjectCount.Text = str(w.GDIObjectCount)
		  GDIObjectPeak.Text = str(w.GDIObjectPeak)
		  
		End Sub
	#tag EndMethod


