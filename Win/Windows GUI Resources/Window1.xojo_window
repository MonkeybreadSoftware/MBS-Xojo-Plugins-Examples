#tag Events PushButton3
	#tag Event
		Sub Action()
		  dim u as integer = WindowCount-1
		  for i as integer = u downto 0
		    if window(i) isa window2 then
		      window(i).close
		    end if
		  next
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton4
	#tag Event
		Sub Action()
		  redim pictures(-1)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton5
	#tag Event
		Sub Action()
		  dim p as new Picture(100,100,32)
		  dim g as Graphics = p.Graphics
		  
		  pictures.Append p
		  
		  
		End Sub
	#tag EndEvent
