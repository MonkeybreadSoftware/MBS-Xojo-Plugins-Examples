#tag Events PageSource1
	#tag Event
		Sub EditSource(ByRef Source As String, Location As Integer)
		  
		  dim urlo as string = app.audioFileOGG.URL
		  dim urlm as string = app.audioFileM4V.URL
		  
		  
		  source = "<audio id=""mymusic"" preload=""auto"">"+_
		  "<source src="""+urlo+""" type=""audio/ogg"" />"+_
		  "<source src="""+urlm+""" type=""audio/mpeg"" />Your browser does not support the audio element.</audio>"
		  
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Slider1
	#tag Event
		Sub ValueChanged()
		  me.ExecuteJavaScript("document.getElementById('mymusic').volume="+str(me.Value/100.0)+";")
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Label4
	#tag Event
