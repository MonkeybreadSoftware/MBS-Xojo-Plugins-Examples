#tag Events List
	#tag Event
		Sub Change()
		  PushButton2.Enabled = me.ListIndex>=0
		  PushButton3.Enabled = me.ListIndex>=0
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton2
	#tag Event
		Sub Action()
		  // With all the infrastructure in place, you can finish the user interface.
		  // When the user selects an item in the store, create a payment object
		  // and add it to the payment queue.
		  
		  dim p as SKProductMBS = list.RowTag(List.ListIndex)
		  
		  dim t as new SKPaymentMBS(p)
		  
		  AppStoreModule.PaymentQueue.addPayment t
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton3
	#tag Event
		Sub Action()
		  // If your store offers the ability to purchase more than one of a product,
		  // you can create a single payment and set the quantity property.
		  
		  dim p as SKProductMBS = list.RowTag(List.ListIndex)
		  
		  dim t as new SKMutablePaymentMBS(p)
		  t.quantity = 2
		  
		  AppStoreModule.PaymentQueue.addPayment t
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton4
	#tag Event
		Sub Action()
		  AppStoreModule.PaymentQueue.restoreCompletedTransactions
		  
		End Sub
	#tag EndEvent
#tag EndEvents
