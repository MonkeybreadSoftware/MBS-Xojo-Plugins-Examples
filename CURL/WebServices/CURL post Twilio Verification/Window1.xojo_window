#tag Events TF_Code
	#tag Event
		Sub TextChange()
		  if me.Text.LenB>0 then
		    PB_Verify.Enabled = True
		  else
		    PB_Verify.Enabled = False
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PB_Verify
	#tag Event
		Sub Action()
		  SendVerification
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events TF_PhoneNumber
	#tag Event
		Sub TextChange()
		  if me.Text.LenB>0 then
		    PB_Send.Enabled = True
		  else
		    PB_Send.Enabled = False
		  end if
		End Sub
	#tag EndEvent
