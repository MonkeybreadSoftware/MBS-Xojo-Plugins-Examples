#tag WindowCode
	#tag Method, Flags = &h21
		Private Sub AfterTransfer()
		  
		  ResultText.text="Result: "+str(ErrorCode)
		  if ErrorCode = 56 then
		    ResultText.text = ResultText.text + " no email"
		  end if
		  
		  if ErrorCode = 0 then
		    dim em as new EmailMessage
		    em.Source = curl.OutputData
		    
		    content.Text = em.BodyPlainText
		    
		    dim ed as EmailHeaders = em.Headers
		    
		    dim c as integer = ed.Count-1
		    for i as integer = 0 to c
		      List.AddRow ed.Name(i)
		      List.Cell(List.LastIndex,1)=ed.Value(ed.Name(i))
		    next
		    
		  end if
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub DoDownload()
		  
		  ErrorCode = curl.Perform
		  TransferDone = true
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Prepare()
		  List.DeleteAllRows
		  content.text = ""
		  TransferDone = false
		  
		  curl = new DownloadCURL
		  
		  curl.CollectOutputData = true
		  curl.OptionURL = "pop3://"+Server.Text+"/"+EmailIndex.text
		  curl.OptionUsername = Username.Text
		  curl.OptionPassword = Passwort.Text
		  curl.YieldTime = true
		  curl.OptionVerbose = true
		  
		  // start thread
		  MyThread.run
		  
		  timer1.Mode = 2
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h21
		Private curl As DownloadCURL
	#tag EndProperty

	#tag Property, Flags = &h21
		Private ErrorCode As integer
	#tag EndProperty

	#tag Property, Flags = &h0
		TransferDone As Boolean
	#tag EndProperty


#tag EndWindowCode

#tag Events PushButton1
	#tag Event
		Sub Action()
		  Prepare
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events MyThread
	#tag Event
		Sub Run()
		  DoDownload
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Timer1
	#tag Event
		Sub Action()
		  if curl.message<>"" then
		    ResultText.Text = curl.Message
		    curl.Message = ""
		  end if
		  
		  if TransferDone then
		    AfterTransfer 
		    me.Mode = 0
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
