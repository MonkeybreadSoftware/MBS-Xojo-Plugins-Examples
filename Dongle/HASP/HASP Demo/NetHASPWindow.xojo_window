	#tag Method, Flags = &h0
		Function showresult(service as integer) As boolean
		  dim p1,p2,p3,p4 as integer
		  
		  // For netHASP we need to ask for the error code using the LastStatus Service function
		  
		  CallHASPMBS NETHASP_LASTSTATUS,0,0,0,0,p1,p2,p3,p4
		  
		  // Displaying the errorcode and the error message string
		  
		  resultfield.text=GetHASPErrorStrMBS(p1)
		  
		  p1field.text=str(p1)
		  p2field.text=str(p2)
		  p3field.text=str(p3)
		  p4field.text=str(p4)
		  
		  servicefield.text=str(service)
		  
		  // return true if there was no error
		  
		  return p1=0
		End Function
	#tag EndMethod

