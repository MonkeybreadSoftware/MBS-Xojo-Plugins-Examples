		  CallHASPMemMBS NETHASP_SETCONFIGFILENAME, 0, Prognum, Pass1, Pass2, p1, p2, p3, p4, NewFileName
		  
		  // Displaying Error Code
		  
		  dummy=showresult(NETHASP_SETCONFIGFILENAME)
		  
		End Sub
