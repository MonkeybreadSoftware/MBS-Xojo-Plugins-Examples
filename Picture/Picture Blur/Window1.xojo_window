#tag WindowCode
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  lx = x
		  ly = y
		  
		  Return true
		  
		End Function
	#tag EndEvent

	#tag Event
		Sub MouseDrag(X As Integer, Y As Integer)
		  Dim dx As Integer = x-lx
		  dim dy as integer = y-ly
		  
		  relx = relx + dx
		  rely = rely + dy
		  
		  lx = x
		  ly = y
		  
		  Title = str(relx)+" "+str(rely)
		  
		  Canvas1.invalidate
		  Canvas2.Invalidate
		  
		  'draw Canvas1.Graphics, ein
		  'draw Canvas2.Graphics, mbs
		  
		  
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  dim l as Picture = LogoMBS(5000)
		  
		  dim t1 as integer = ticks
		  
		  // Einhugur Plugins
		  
		  const HaveEinhugurPlugins = false
		  
		  #if HaveEinhugurPlugins
		    dim e as new GaussianBlur(4)
		    dim q as Picture = e.applyEffect(l)
		    
		    t1 = ticks - t1
		    
		    EIN = q
		  #endif
		  
		  dim t2 as integer = ticks
		  
		  // MBS Plugins
		  dim d as Picture = l.BlurMBS(2)
		  t2 = ticks - t2
		  
		  MBS = d
		  Title = str(t1)+" "+str(t2)
		  
		  RelX = -1000
		  RelY = -1000
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub draw(g as Graphics, p as Picture)
		  if p<>nil then
		    g.DrawPicture p, RelX, RelY
		  end if
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		EIN As Picture
	#tag EndProperty

	#tag Property, Flags = &h0
		lx As Integer
	#tag EndProperty

	#tag Property, Flags = &h0
		ly As Integer
	#tag EndProperty

	#tag Property, Flags = &h0
		MBS As Picture
	#tag EndProperty

	#tag Property, Flags = &h0
		RelX As Integer
	#tag EndProperty

	#tag Property, Flags = &h0
		RelY As Integer
	#tag EndProperty


#tag EndWindowCode

#tag Events Canvas1
	#tag Event
		Sub Paint(g As Graphics, areas() As REALbasic.Rect)
		  draw g,ein
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Canvas2
	#tag Event
