#tag Events PushButton2
	#tag Event
		Sub Action()
		  dim w as WebViewMBS = HTMLViewer1.WebViewMBS
		  dim d as WebDataSourceMBS = w.mainFrame.dataSource
		  dim a as WebArchiveMBS = d.webArchive
		  
		  dim f as FolderItem = SpecialFolder.Desktop.Child("WebArchive test.webarchive")
		  dim b as BinaryStream = BinaryStream.Create(f)
		  b.Write a.data
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton3
