#tag EndEvents
#tag Events PushButton4
	#tag Event
		Sub Action()
		  dim f as FolderItem = SpecialFolder.Desktop.Child("WebArchive test.webarchive")
		  dim b as BinaryStream = BinaryStream.Open(f)
		  dim s as string = b.Read(b.Length)
		  dim m as MemoryBlock = s
		  dim a as WebArchiveMBS = new WebArchiveMBS(m)
		  
		  
		  
		  
		  dim w as WebViewMBS = HTMLViewer1.WebViewMBS
		  w.mainFrame.loadArchive a
		  
		End Sub
	#tag EndEvent
