#tag WindowCode
	#tag Event
		Sub Open()
		  
		  wui=new MyFrameEvents
		  
		  web.InstallWebFrameLoadDelegateMBS wui
		  
		  if not me.Composite then
		    MsgBox "This window must be composite!"
		  end if
		  
		  run
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h1
		Protected Sub run()
		  web.LoadHTMLStringMBS EditField1.text,"text/html","","http://www.monkeybreadsoftware.de"
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h1
		Protected wui As myFrameEvents
	#tag EndProperty


#tag EndWindowCode

#tag Events EditField1
	#tag Event
		Sub TextChange()
		  UpdateTimer.mode=1
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events UpdateTimer
