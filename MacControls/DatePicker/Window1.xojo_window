#tag Events QueryButton
	#tag Event
		Sub Action()
		  dim d1 as date = n1.DateValue
		  dim d2 as date = n2.DateValue
		  dim d3 as date = n3.DateValue
		  dim d4 as date = n4.DateValue
		  dim d5 as date = n5.DateValue
		  
		  MsgBox _
		  d1.LongDate+" "+d1.LongTime+EndOfLine+_
		  d2.LongDate+" "+d2.LongTime+EndOfLine+_
		  d3.LongDate+" "+d3.LongTime+EndOfLine+_
		  d4.LongDate+" "+d4.LongTime+EndOfLine+_
		  d5.LongDate+" "+d5.LongTime
		  
		  
		End Sub
