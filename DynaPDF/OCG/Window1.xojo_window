#tag WindowCode
	#tag Event
		Sub Open()
		  file = findFIle("test.pdf")
		  if file = nil then
		    file = GetOpenFolderItem("")
		  end if
		  
		  ReadInfo
		  
		  update
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function FindFile(name as string) As FolderItem
		  // Look for file in parent folders from executable on
		  
		  dim parent as FolderItem = app.ExecutableFile.Parent
		  
		  while parent<>Nil
		    
		    dim file as FolderItem = parent.Child(name)
		    
		    if file<>Nil and file.Exists then
		      Return file
		    end if
		    
		    parent = parent.Parent
		  wend
		  
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub ReadInfo()
		  p = new MyDynaPDFMBS
		  
		  p.SetLicenseKey "Pro" // For this example you can use Pro or Enterprise License
		  
		  call p.CreateNewPDF(nil)
		  
		  // Skip anything that is not required
		  call p.SetImportFlags p.kifImportAll+p.kifImportAsPage
		  
		  // From which PDF file do you want to extract the images?
		  call p.OpenImportFile(file, p.kptOpen, "")
		  
		  // import pages
		  call p.ImportPDFFile(1, 1.0, 1.0)
		  
		  call p.CloseImportFile
		  
		  // lookup all links
		  dim u as integer = p.GetOCGCount - 1
		  Title = str(p.GetOCGCount)+" OCGs"
		  
		  for i as integer = 0 to u
		    dim b as DynaPDFOCGMBS = p.GetOCG(i)
		    
		    List.AddRow b.Name
		    List.RowTag(List.LastIndex) = i
		    
		  next
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Update()
		  dim u as integer = List.ListCount-1
		  for i as integer = 0 to u
		    
		    dim on as Boolean = List.CellCheck(i,0)
		    dim h as integer = list.RowTag(i)
		    
		    call p.SetOCGState h, on, false
		    
		  next
		  
		  #if RBVersion > 2013 then
		    if self.BackingScaleFactorMBS > 1 then
		      // Mac retina 
		      pic = p.RenderPagePicture(1, output.Width*2, output.Height*2)
		    else
		      // normal
		      pic = p.RenderPagePicture(1, output.Width, output.Height)
		    end if
		    
		  #else
		    pic = p.RenderPagePicture(1, output.Width, output.Height)
		  #endif
		  
		  output.Invalidate
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
