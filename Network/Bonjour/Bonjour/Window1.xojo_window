#tag WindowCode
	#tag Event
		Sub Close()
		  b = nil
		  e = nil
		  r = nil
		  l = nil
		  
		  
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  
		  if MyBonjourBrowseMBS.Available then
		    write "Bonjour is available."
		  else
		    msgBox "No Bonjour found!"
		    quit
		  end if
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub write(s as string)
		  listBox1.addrow s
		  listBox1.scrollPosition=listBox1.listcount
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		b As myBonjourbrowseMBS
	#tag EndProperty

	#tag Property, Flags = &h0
		e As myBonjourdomainEnumerationMBS
	#tag EndProperty

	#tag Property, Flags = &h0
		l As myBonjourResolveMBS
	#tag EndProperty

	#tag Property, Flags = &h0
		r As myBonjourregisterMBS
	#tag EndProperty


#tag EndWindowCode

#tag Events TypelistButton
	#tag Event
		Sub Action()
		  // URL from Apple's docs
		  showurl "http://www.iana.org/assignments/port-numbers"
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events RegisterButton
	#tag Event
		Sub Action()
		  r = new MyBonjourRegisterMBS
		  
		  if r.Register(0, MyBonjourBrowseMBS.kInterfaceIndexAny, Rservicename.text, Rservicetype.text, Rdomain.text, "", val(Rport.text),"") then
		    MsgBox "Registering started successfull."
		  else
		    MsgBox "Registering failed."
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ResolveButton
	#tag Event
		Sub Action()
		  l=new MyBonjourResolveMBS
		  
		  if l.Resolve(0, lServicename.text,lservicetype.text, ldomain.text) then
		    MsgBox "Resolve started successfull."
		  else
		    MsgBox "Resolve failed."
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton1
	#tag Event
		Sub Action()
		  b=new MyBonjourBrowseMBS
		  
		  if b.Browse(0,bServicename.text,bdomain.text) then
		    MsgBox "Browse started successfull."
		  else
		    MsgBox "Browse failed."
		  end if
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton2
	#tag Event
		Sub Action()
		  dim n as integer 
		  
		  if RadioEnumerate(0).Value then
		    n=MyBonjourDomainEnumerationMBS.kFlagsRegistrationDomains // recommended registration domains
		  else
		    n=MyBonjourDomainEnumerationMBS.kFlagsBrowseDomains // browsing domains
		  end if
		  
		  e=new MyBonjourDomainEnumerationMBS
		  
		  if e.EnumerateDomains(n,0) then
		    MsgBox "Enumerating started successfull."
		  else
		    MsgBox "Enumerating failed."
		  end if
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton3
	#tag Event
		Sub Action()
		  r=nil
		  
		End Sub
	#tag EndEvent
#tag EndEvents
