#tag WindowCode
	#tag Event
		Sub Open()
		  
		  if not CocoaControlMBS1.Available then
		    MsgBox "This example requires Mac OS X 10.5."
		    quit
		  end if
		  
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h0
		n As NSTextViewMBS
	#tag EndProperty

	#tag Property, Flags = &h0
		scrollview As nsscrollViewMBS
	#tag EndProperty


#tag EndWindowCode

#tag Events CocoaControlMBS1
	#tag Event
		Function GetView() As NSViewMBS
		  scrollview=new NSScrollViewMBS(0,0,me.Width,me.Height)
		  
		  n=new NSTextViewMBS(0,0,me.Width,me.Height)
		  n.ContinuousSpellCheckingEnabled=true
		  n.allowsUndo=true
		  
		  // tell it how the resize is going
		  n.autoresizingMask=n.NSViewHeightSizable+n.NSViewWidthSizable
		  
		  scrollview.documentView=n
		  scrollview.hasVerticalScroller=true
		  scrollview.hasHorizontalScroller=True
		  Return scrollview
		  
		  
		End Function
	#tag EndEvent
#tag EndEvents
