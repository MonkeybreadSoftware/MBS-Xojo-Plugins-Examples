#tag Events PushButton3
	#tag Event
		Sub Action()
		  dim tt,t as integer
		  dim f as DirectorySizeMBS
		  
		  tt=val(PopupMenu1.text)
		  
		  t=ticks
		  f=SpecialFolder.System.CalculateDirectorySizeMBS(true,tt)
		  t=ticks-t
		  
		  uTime.text=Format(t,"0")+" ticks"
		  if f<>Nil then
		    uLData.text=Format(f.LogicalDataForkSize,"0")+" bytes"
		    uLRes.text=Format(f.LogicalResourceForkSize,"0")+" bytes"
		    uLTotal.text=Format(f.LogicalTotalSize,"0")+" bytes"
		    
		    uPData.text=Format(f.PhysicalDataForkSize,"0")+" bytes"
		    uPRes.text=Format(f.PhysicalResourceForkSize,"0")+" bytes"
		    uPTotal.text=Format(f.PhysicalTotalSize,"0")+" bytes"
		    
		    uFiles.text=Format(f.FilesCount,"0")
		    uFolders.text=Format(f.FolderCount,"0")
		  end if
		End Sub
	#tag EndEvent
