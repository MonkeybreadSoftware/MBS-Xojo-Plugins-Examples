		Sub Action()
		  dim t as integer
		  dim d,r as Double
		  dim fi,fo as integer
		  
		  d=0
		  r=0
		  
		  t=ticks
		  dir1 SpecialFolder.System,d,r,fi,fo
		  t=ticks-t
		  
		  pTime.text=Format(t,"0")+" ticks"
		  pData.text=Format(d,"0")+" bytes"
		  pRes.text=Format(r,"0")+" bytes"
		  pTotal.text=Format(r+d,"0")+" bytes"
		  
		  pFiles.text=Format(fi,"0")
		  pFolders.text=Format(fo,"0")
		  
		End Sub
