		Sub Close()
		  dis = nil
		End Sub
	#tag EndEvent

	#tag Event
		Sub Open()
		  if not ResolutionLibraryPresentMBS then
		    msgBox "The library RezLib was not found. Please place this library into the Realbasic or after compiling into your application folder."
		  end if
		  
		  dis = GetDisplayMBS(0)
		  
		End Sub
	#tag EndEvent


