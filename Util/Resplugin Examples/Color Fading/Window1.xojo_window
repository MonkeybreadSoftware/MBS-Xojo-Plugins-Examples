#tag Events PushButton1
	#tag Event
		Sub Action()
		  dis.FadeGammaTo(0, RGB(Val(r1.Text), Val(g1.Text), Val(b1.Text)), 500)
		  dis.FadeGammaTo(0, RGB(Val(r2.Text), Val(g2.Text), Val(b2.Text)), 2000)
		  dis.FadeGammaTo(1000, RGB(0, 0, 0), 500)
		End Sub
	#tag EndEvent
