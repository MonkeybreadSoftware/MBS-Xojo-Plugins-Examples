	#tag Event
		Sub Action()
		  ' Disabled because it's dangerous
		  
		  if TargetWin32 then
		    dim e as integer
		    
		    e=process.KillProcess
		    Select case e
		    case 0
		      MsgBox "Can't kill the process."
		    case 1
		      MsgBox "Killed the process well."
		    case 1
		      MsgBox "Killed the process badly."
		    else
		      MsgBox "unknown result code."
		    end Select
		    
		  else
		    if process.KillProcess=0 then
		      MsgBox "Killed the process."
		    else
		      MsgBox "Can't kill the process. "+str(process.Lasterror)
		    end if
		  end if
		End Sub
