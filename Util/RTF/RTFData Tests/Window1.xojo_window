	#tag Method, Flags = &h0
		Sub Run()
		  dim RTFData1 as string = TextArea1.StyledText.RTFData
		  TextArea2.Text = RTFData1
		  TextArea3.StyledText.RTFData = RTFData1
		  
		  dim RTFData2 as string = TextArea1.StyledText.RTFDataMBS
		  TextArea5.Text = RTFData2
		  TextArea6.StyledText.RTFDataMBS = RTFData2
		  
		  
		  if StrCompBytesMBS(RTFData2, RTFData1) = 0 then
		    label6.Text = "Equal"
		  else
		    label6.Text = "Not equal."
		  end if
		  
		  dim RTFData3 as string = TextArea6.StyledText.RTFDataMBS
		  
		  if StrCompBytesMBS(RTFData2, RTFData3) = 0 then
		    label7.Text = "Equal"
		  else
		    label7.Text = "Not equal."
		  end if
		  
		  TextArea7.text = RTFData3
		End Sub
	#tag EndMethod


