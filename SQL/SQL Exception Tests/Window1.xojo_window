		    SQLGlobalsMBS.RaiseException "Hello World"
		    
		  catch exc as RuntimeException
		    
		    msgBox "Exception:"+str(exc.ErrorNumber)+endOfLine+exc.Message+" ("+str(exc.Message.len)+")"
		    
		    
		  end try
		  
		End Sub
	#tag EndEvent
