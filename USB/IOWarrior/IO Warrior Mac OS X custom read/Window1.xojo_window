#tag WindowCode
	#tag Event
		Sub Open()
		  dim i,c as integer
		  dim d as IOWarriorCarbonDeviceMBS
		  
		  if TargetMachO then
		    
		  else
		    MsgBox "Please use only with MachO targets on Mac OS X!"
		    quit
		  end if
		  
		  io=new IOWarriorCarbonMBS
		  
		  // custom IDs can be used
		  'io.USBVendorID=&h1466
		  'io.USB24DeviceID=&h3000
		  
		  init.text= str(io.Init)+" init"
		  present.text= str(io.IsPresent)+" present"
		  
		  c=io.CountInterfaces-1
		  for i=0 to c
		    d=io.InterfaceAtIndex(i)
		    ListBox1.AddRow hex(d.InterfaceHandle)
		    ListBox1.cell(ListBox1.LastIndex,1)=d.SerialNumber
		    ListBox1.Cell(ListBox1.LastIndex,2)=str(d.InterfaceType)
		  next
		  
		End Sub
	#tag EndEvent


	#tag Property, Flags = &h1
