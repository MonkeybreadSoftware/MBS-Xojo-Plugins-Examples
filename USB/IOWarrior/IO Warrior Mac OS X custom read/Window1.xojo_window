#tag Events Timer1
	#tag Event
		Sub Action()
		  dim ReportID,n as integer
		  dim v as integer
		  dim d as date
		  dim m as MemoryBlock
		  dim s as string
		  
		  ReportID=0
		  
		  // kIOWarrior24Interface0 => 2 Byte per Report
		  // kIOWarrior24Interface1 => 7 Byte
		  
		  m=NewMemoryBlock(20)
		  
		  s=""
		  n=io.ReadFromInterface(io.FirstInterfaceOfType(io.kIOWarrior24Interface0),0,4,m)
		  if n=0 then
		    s=hex(m.Long(0))+" "
		  else
		    'read.text="Error "+str(n)
		  end if
		  
		  n=io.ReadFromInterface(io.FirstInterfaceOfType(io.kIOWarrior24Interface1),0,7,m)
		  if n=0 then
		    s=s+hex(m.Long(0))+" "+hex(m.Long(4))
		  else
		    'read.text="Error "+str(n)
		  end if
		  
		  read.text=s
		End Sub
	#tag EndEvent
#tag EndEvents
