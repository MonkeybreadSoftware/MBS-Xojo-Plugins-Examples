	#tag Method, Flags = &h1
		Protected Function converttext(t as string) As string
		  dim u as string=ConvertEncoding(t,encodings.UTF8)
		  dim a,i,c as integer
		  dim s as string
		  
		  // Phidget uses: ASCII Standard Character Set with Katakana Extension
		  // so we translate here a few characters manually
		  
		  c=len(u)
		  for i=1 to c
		    a=asc(mid(u,i,1))
		    
		    // äöüßµ–ñ∞∃αβρΣΩ÷¢
		    
		    Select case a
		    case asc("ä")
		      s=s+chrb(225)
		    case asc("ß")
		      s=S+chrb(226)
		    case asc("€")
		      s=S+chrb(227)
		    case asc("µ")
		      s=S+chrb(228)
		    case asc("ñ")
		      s=S+chrb(238)
		    case asc("ö")
		      s=S+chrb(239)
		    case asc("∞")
		      s=S+chrb(&hF3)
		    case asc("π")
		      s=S+chrb(&h7F)
		    case asc("ü")
		      s=S+chrb(&hF5)
		    case asc("–")
		      s=S+chrb(&hB0)
		    case asc("∃")
		      s=s+chrb(&hD6)
		    case asc("α")
		      s=S+chrb(&hE0)
		    case asc("β")
		      s=S+chrb(&hE2)
		    case asc("ρ")
		      s=S+chrb(&hE6)
		    case asc("Σ")
		      s=s+chrb(&hF6)
		    case asc("Ω")
		      s=s+chrb(&hF4)
		    case asc("÷")
		      s=S+chrb(&hFD)
		    case asc("¢")
		      s=s+chrb(&hEC)
		    else
		      s=s+chrb(a)
		    end Select
		    
		  next
		  
		  return s
		End Function
	#tag EndMethod

