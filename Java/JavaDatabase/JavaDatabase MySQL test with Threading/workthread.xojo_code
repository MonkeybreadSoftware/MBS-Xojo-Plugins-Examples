#tag Class
Protected Class workthread
Inherits thread
	#tag Event
		Sub Run()
		  dim i as integer
		  dim s as string
		  
		  for i=1 to 10000
		    
		    window1.InsertCounter=window1.InsertCounter+1
		    
		    s="INSERT INTO myTable(test_id, test_val,test_val2) VALUES("+str(window1.InsertCounter)+",'HelloWorld','HelloWorld')"
		    
		    window1.j.MyExecuteSQL s
		  next
		  
		  window1.List.AddRow "thread finished"
		  
		  window1.j.vm.FreeCurrentThread
		  
		End Sub
	#tag EndEvent


