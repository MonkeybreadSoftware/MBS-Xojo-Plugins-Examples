#tag Events Timer1
	#tag Event
		Sub Action()
		  if d<>Nil then
		    
		    dim r as JavaRuntimeMBS = d.vm.Runtime
		    
		    StaticText1.text="RuntimeFreeMemory: "+str(r.FreeMemory)+EndOfLine+"RuntimeMaxMemory: "+str(r.MaxMemory)+EndOfLine+"RuntimeTotalMemory: "+str(r.TotalMemory)
		  end if
		  
		End Sub
