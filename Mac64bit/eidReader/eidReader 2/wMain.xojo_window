	#tag Event
		Sub Action()
		  Me.Enabled = False
		  Me.CaptionDelta = -3
		  pwRC.Visible = True
		  
		  slotManager = New scsm
		  
		  slotManager.OpenFirstSlot
		  lblOSlot.Text = slotManager.SlotID
		  
		  tmrCard.Period = 5000
		  tmrCard.Mode = Timer.ModeSingle
		  tmrCard.Enabled = True
		  
		  
		  
		  
		  
		End Sub
	#tag EndEvent
