#tag WindowCode
	#tag Event
		Sub Resized()
		  resize
		End Sub
	#tag EndEvent

	#tag Event
		Sub Resizing()
		  resize
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub resize()
		  dim w as integer = (Width-20-20-14)/2
		  
		  listbox1.Width = w
		  canvas1.Width =W
		  canvas1.left = w+20+14
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		downloaders(-1) As Variant
	#tag EndProperty


#tag EndWindowCode

#tag Events StartButton
	#tag Event
		Sub Action()
		  dim request as new NSURLRequestMBS(iURL.Text)
		  dim error as NSErrorMBS
		  dim response as NSURLResponseMBS
		  dim d as MemoryBlock = NSURLConnectionMBS.sendSynchronousRequest(request, response, error)
		  
		  if d<>Nil then
		    dim pic as Picture = JPEGStringToPictureMBS(d, true)
		    
		    canvas1.Backdrop = pic
		  else
		    canvas1.Backdrop = nil
		  end if
		  
		  if error<>Nil then
		    MsgBox "Error: "+error.description
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events StartButton1
	#tag Event
		Sub Action()
		  dim r as new NSURLRequestMBS(iURL.text)
		  dim m as new MyNSURLConnectionMBS(r)
		  
		  m.list = Listbox1
		  m.start
		  
		  downloaders.Append m
		  
		End Sub
	#tag EndEvent
#tag EndEvents
