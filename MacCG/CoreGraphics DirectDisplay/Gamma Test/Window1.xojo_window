#tag WindowCode
	#tag Event
		Sub Open()
		  dim e as integer
		  dim f as CGDisplayTransferFormulaMBS
		  
		  display = CGDisplayMBS.MainDisplay
		  
		  e=display.GetDisplayTransferFormula(f)
		  form=f
		  LastError.text=str(e)
		  
		  RedMax.Value=f.RedMax*100
		  RedMin.Value=f.RedMin*100
		  RedGamma.Value=f.RedGamma*100
		  GreenMin.value=f.GreenMin*100
		  GreenMax.Value=f.GreenMax*100
		  GreenGamma.Value=f.GreenGamma*100
		  BlueGamma.Value=f.BlueGamma*100
		  BlueMax.Value=f.BlueMax*100
		  BlueMin.Value=f.BlueMin*100
		  
		  inited=true
		  
		End Sub
	#tag EndEvent


