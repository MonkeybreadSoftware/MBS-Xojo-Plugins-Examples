#tag WindowCode
	#tag Event
		Sub Open()
		  
		  Title = RBVersionString
		  
		  if TargetCarbon then
		    Title = Title + " Carbon"
		  else
		    Title = Title + " Cocoa"
		  end if
		  
		  dim p as Picture = LogoMBS(200, true)
		  
		  canvas1.Backdrop = p
		  
		  dim n as new NSImageMBS(p)
		  canvas2.Backdrop = n.CopyPictureWithMask
		  
		  dim c as CGImageMBS = CGCreateImageMBS(p)
		  canvas3.Backdrop = c.Picture
		  
		  p = LogoMBS(200)
		  dim g as Graphics = p.Mask.Graphics
		  
		  g.ForeColor = &cFFFFFF
		  g.FillRect 0,0,200,200
		  
		  g.ForeColor = &c000000
		  g.FillOval 0,0,200,200
		  
		  canvas4.Backdrop = p
		  
		  n = new NSImageMBS(p)
		  canvas5.Backdrop = n.CopyPictureWithMask
		  
		  
		  c = CGCreateImageMBS(p)
		  canvas6.Backdrop = c.Picture
		  
		  p = LogoMBS(200)
		  dim m as new Picture(200,200,32)
		  g = m.Graphics
		  
		  g.ForeColor = &cFFFFFF
		  g.FillRect 0,0,200,200
		  
		  g.ForeColor = &c000000
		  g.FillOval 0,0,200,200
		  
		  canvas7.Backdrop = p
		  
		  n = new NSImageMBS(p,m)
		  canvas8.Backdrop = n.CopyPictureWithMask
		  
		  c = CGCreateImageMBS(p,m)
		  canvas9.Backdrop = c.Picture
		  
		End Sub
	#tag EndEvent


